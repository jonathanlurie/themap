{
  "name": "themap",
  "version": "1.0.0",
  "description": "An app to make map posters",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . 'The Map' --overwrite --plateform=darwin --out=$HOME --icon=asset/icon.icns; echo 'Package built in $HOME'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanlurie/themap.git"
  },
  "keywords": [
    "map",
    "poster",
    "design"
  ],
  "author": "Jonathan Lurie",
  "license": "MIT",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-packager": "^8.7.2"
  },
  "dependencies": {
    "fs-sync": "^1.0.4",
    "gm": "^1.23.0",
    "imagemagick-darwin-static": "^1.0.11",
    "jsonfile": "^3.0.1",
    "leaflet": "^1.1.0"
  }
}
